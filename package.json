{
  "name": "silverload",
  "homepage": "https://github.com/dochevviktor/silverload",
  "version": "5.1.3",
  "private": true,
  "author": "Viktor Dochev",
  "description": "A thin and lite Image Browser app",
  "repository": "github:dochevviktor/silverload",
  "main": "build/main.js",
  "scripts": {
    "start:dev": "webpack serve && rimraf build",
    "build:prod": "rimraf out && webpack --env production && yarn electron:package && rimraf build",
    "build:pub": "rimraf out && webpack --env production && yarn electron:publish && rimraf build",
    "electron:install": "electron-rebuild",
    "electron:package": "ts-node config/misc/package.config.ts",
    "electron:publish": "ts-node config/misc/publish.config.ts",
    "clean:output": "rimraf out",
    "lint": "run-p -n -l lint:eslint lint:stylelint",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:stylelint": "stylelint 'src/**/*.scss'",
    "lint:fix:eslint": "eslint '**/*.{ts,tsx}' --fix",
    "lint:fix:stylelint": "stylelint 'src/**/*.scss' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,scss}'",
    "document:build": "ts-node config/misc/doc.config.ts"
  },
  "precommit": {
    "run": "lint:fix:eslint, lint:fix:stylelint, format, document:build"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@ffmpeg/ffmpeg": "^0.12.10",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@preact/compat": "^17.1.2",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/node": "^22.0.0",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.20",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@types/webpack-env": "^1.18.5",
    "@types/webpack-merge": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "antd": "4.24.16",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "electron": "31.3.0",
    "electron-builder": "^24.13.3",
    "electron-builder-squirrel-windows": "^22.14.13",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "file-type": "^19.3.0",
    "html-inline-css-webpack-plugin": "^1.11.2",
    "html-minimizer-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "inline-chunk-html-plugin": "^1.1.1",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "object-hash": "^3.0.0",
    "pre-commit": "^1.2.2",
    "preact": "^10.23.1",
    "prettier": "3.3.3",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.2.2",
    "replace-in-file": "^8.1.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.43.3",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^16.7.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-scss": "^6.4.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0",
    "webpack": "5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "6.0.1"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.12.6",
    "dexie": "4.0.8"
  }
}
