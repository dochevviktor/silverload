    "stylelint-scss": "^3.20.1",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "9.2.6",
    "ts-node": "^10.3.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2",
