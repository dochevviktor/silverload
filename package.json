{
  "name": "silverload",
  "homepage": "https://github.com/dochevviktor/silverload",
  "version": "5.1.1",
  "private": true,
  "author": "Viktor Dochev",
  "description": "A thin and lite Image Browser app",
  "repository": "github:dochevviktor/silverload",
  "main": "build/main.js",
  "scripts": {
    "start:dev": "webpack serve",
    "build:prod": "rimraf out & webpack --env production & yarn electron:package & rimraf build",
    "build:pub": "rimraf out & webpack --env production & yarn electron:publish & rimraf build",
    "electron:install": "electron-rebuild",
    "electron:package": "ts-node config/misc/package.config.ts",
    "electron:publish": "ts-node config/misc/publish.config.ts",
    "clean:output": "rimraf out",
    "lint": "run-p -n -l lint:eslint lint:stylelint",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:stylelint": "stylelint \"src/**/*.scss\"",
    "lint:fix:eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:fix:stylelint": "stylelint \"src/**/*.scss\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss}\"",
    "document:build": "ts-node config/misc/doc.config.ts"
  },
  "precommit": {
    "run": "lint:fix:eslint, lint:fix:stylelint, format, document:build"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@ffmpeg/ffmpeg": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
